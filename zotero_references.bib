  keywords = {2005,Animal Learning,medial temporal lobe,Memory,relational learning,Spatial Memory,spatial navigation,temporal context model,Temporal Lobe},
  file = {EBSCO Full Text:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/P2JU9I3N/Howard et al. - 2005 - The Temporal Context Model in Spatial Navigation a.pdf:application/pdf},
