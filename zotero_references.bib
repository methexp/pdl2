  note = {00008 PMID: 9231149},
  file = {Houwer_1997.pdf:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/NX8MJGHU/Houwer_1997.pdf:application/pdf}
}

