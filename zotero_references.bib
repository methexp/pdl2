}

@article{graf_process_1994,
  title = {Process Dissociation Procedure: {{Handle}} with Caution!},
  volume = {6},
  issn = {0954-1446},
  url = {http://www.tandfonline.com/doi/abs/10.1080/09541449408520139},
  doi = {10.1080/09541449408520139},
  shorttitle = {Process Dissociation Procedure},
  abstract = {Abstract The process dissociation procedure (PDP) was introduced as a general-purpose tool for quantifying the extent to which performance on various cognitive tasks is mediated by consciously controlled versus automatic processes. In this paper, we consider several aspects that limit the procedure's domain of application. We suggest that the PDP is inappropriate for studying performance of implicit versus explicit memory tests because its focus on conscious versus automatic processes is different from the distinction between intentional versus non-intentional remembering that is targeted by implicit and explicit memory tests. We point out that the complexity of the instructions that are required for the procedure makes it unsuitable for use with special populations, such as amnesic patients. An examination of two core assumptions of the procedure (invariance of recollection and invariance of familiarity) revealed evidence suggesting that these assumptions are violated in many circumstances. In a final section, we introduce a more general model for quantifying consciously controlled versus automatic retrieval processes and show that the original PDP formulation is a special case that is obtained by making simplifying but probably unjustified assumptions. We hope that this paper will stimulate the development of more advanced and alternative investigative methods for dissociating processes.},
  timestamp = {2014-03-28T13:05:29Z},
  number = {2},
  journaltitle = {European Journal of Cognitive Psychology},
  author = {Graf, Peter and Komatsu, Shin-Ichi},
  urldate = {2014-03-26},
  date = {1994},
  pages = {113--129},
  note = {00157},
  file = {Graf_etal_1994.pdf:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/ZHQWJ2ID/Graf_etal_1994.pdf:application/pdf}
}

@article{fisk_highly_2013,
