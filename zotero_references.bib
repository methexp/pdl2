  pages = {297--305},
  keywords = {Cognitive Psychology,Neurosciences},
  file = {Full Text PDF:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/RVP5SHRI/Bierman et al. - 2005 - Intuitive decision making in complex situations S.pdf:application/pdf}
