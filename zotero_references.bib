  doi = {10.1016/j.concog.2011.09.020},
  shorttitle = {Out of Control},
  abstract = {The demonstration of a sequential congruency effect in sequence learning has been offered as evidence for control processes that act to inhibit automatic response tendencies (Jiménez, Lupiáñez, \&amp; Vaquero, 2009) via unconscious conflict monitoring. Here we propose an alternative interpretation of this effect based on the associative learning of chains of sequenced contingencies. This account is supported by simulations with a Simple Recurrent Network, an associative (connectionist) model of sequence learning. We argue that the control- and associative-based accounts differ in their predictions concerning the magnitude of the sequential congruency effect across training. These predictions are tested by reanalysing data from a study by Shanks, Wilkinson, and Channon (2003). The results support the associative learning account which explains the sequential congruency effect without appealing to control processes (either conscious or unconscious).},
  timestamp = {2014-10-30T08:46:49Z},
  number = {1},
  journaltitle = {Consciousness and Cognition},
  shortjournal = {Consciousness and Cognition},
  series = {Beyond the Comparator Model},
  author = {Beesley, Tom and Jones, Fergal W. and Shanks, David R.},
  urldate = {2014-10-29},
  date = {2012-03},
  pages = {413--421},
  keywords = {Associative learning,Connectionism,control,Sequential congruency},
  file = {ScienceDirect Full Text PDF:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/RDPVH8A6/Beesley et al. - 2012 - Out of control An associative account of congruen.pdf:application/pdf}
}

