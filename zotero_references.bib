  volume = {144},
  doi = {10.1037/xge0000044},
  shorttitle = {The {{Invariance Assumption}} in {{Process}}-{{Dissociation Models}}},
  abstract = {The class of process-dissociation models, a subset of the class of multinomial processing-tree models, is one of the best understood classes of models used in experimental psychology. A number of prominent debates have addressed fundamental assumptions of process-dissociation models, leading, in many cases, to conceptual clarifications and extended models that address identified issues. One issue that has so far defied empirical clarification is how to evaluate the invariance assumption for the dominant process. Violations of the invariance assumption have, however, the potential to bias conventional process-dissociation analyses in different ways, and they can cause misleading theoretical interpretations and conclusions. Based on recent advances in multinomial modeling, we propose new approaches to examine the invariance assumption empirically and apply them in 6 studies to 3 prominent fields of application of process-dissociation models: to the Stroop task, to the interplay of recollection and habit in cued recall, and to the study of racial bias in the weapon task. In each of these content domains, the invariance assumption is found to be violated to a considerable extent.},
  timestamp = {2017-07-14T17:17:32Z},
  number = {1},
  journaltitle = {Journal of Experimental Psychology: General},
  author = {Klauer, Karl Christoph and Dittrich, Kerstin and Scholtes, Christine and Voss, Andreas},
  date = {2015-02},
  pages = {198--221},
  keywords = {*Cognitive Processes,*Dissociation,*Experimental Psychology,*Mental Models,Cognitive Processes,Cued Recall,dissociation,Experimental Psychology,Habits,Memory,Mental Models,multinomial processing-tree models,process dissociation models,Racial and Ethnic Attitudes,Response Bias,Stroop effect,weapon task},
  file = {[PDF] von uni-heidelberg.de:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/4P9TV6TA/Klauer et al. - The Invariance Assumption in Process-Dissociation .pdf:application/pdf;EBSCO Full Text:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/PR8AB8TI/Klauer et al. - 2015 - The invariance assumption in process-dissociation .pdf:application/pdf},
  groups = {pdl1}
}

