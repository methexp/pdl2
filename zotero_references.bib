  urldate = {2015-02-11},
  date = {2012-03},
  pages = {440--456},
  keywords = {chaining,Implicit Memory,implicit sequence learning,Reaction Time,Sequential Learning,serial order,serial reaction time,Short Term Memory,SRT,working memory},
  file = {EBSCO Full Text:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/IBVP4CXA/Schuck et al. - 2012 - Position–item associations play a role in the acqu.pdf:application/pdf}
