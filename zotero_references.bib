  date = {2001-06-01},
  pages = {343--350},
  note = {00341},
  keywords = {Cognitive Psychology},
  file = {Destrebecqz_etal_2001.pdf:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/CAQF42N8/Destrebecqz_etal_2001.pdf:application/pdf;Full Text PDF:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/FC2RMB3V/Destrebecqz und Cleeremans - 2001 - Can sequence learning be implicit New evidence wi.pdf:application/pdf},
