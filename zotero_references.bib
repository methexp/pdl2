  date = {2005-10},
  pages = {383--402},
  note = {00025},
  keywords = {Associative chaining,Associative symmetry,Paired associates learning,Positional coding,Serial order memory},
  file = {ScienceDirect Full Text PDF:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/HRTG7N49/Caplan - 2005 - Associative isolation Unifying associative and li.pdf:application/pdf},
