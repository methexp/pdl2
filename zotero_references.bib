@incollection{goschke_implicit_1998,
  location = {{Thousand Oaks,  CA,  US}},
  title = {Implicit Learning of Perceptual and Motor Sequences: {{Evidence}} for Independent Learning Systems},
  rights = {(c) 2012 APA, all rights reserved},
  isbn = {0-7619-0197-3 (Hardcover)},
  shorttitle = {Implicit Learning of Perceptual and Motor Sequences},
  abstract = {In this chapter, I focus on 3 questions: 1. Is the knowledge acquired in serial reaction (SRT) tasks actually unconscious? 2. What kind of knowledge is acquired in SRT tasks? 3. What brain systems underlie implicit sequence learning? In the first part, I discuss methodological problems in measuring explicit and implicit knowledge and review evidence on whether sequence learning is or is not implicit, including some of our own recent research, in which we applied the process-dissociation method to separate explicit and implicit knowledge. In the 2nd part of the chapter, I defend the hypothesis that learning in SRT tasks is not restricted to the acquisition of sequences of motor responses, but that various types of stimulus sequences can be implicitly learned, even when there is no structured sequence of overt responses. In the 3rd section, I discuss neuropsychological, brain imaging, and electrophysiological evidence suggesting that different domain-specific brain systems are involved in sequence learning. In the final section, I briefly discuss whether, in addition to domain-specific sequence-representation systems, there is also a domain-unspecific sequence-acquisition device storing sequences in an abstract format. I conclude with some speculative remarks on the functional properties of implicit and explicit learning.},
  timestamp = {2014-11-12T11:15:43Z},
  booktitle = {Handbook of Implicit Learning},
  publisher = {{Sage Publications, Inc}},
  author = {Goschke, Thomas},
  editor = {Stadler, M. A. and Frensch, P. A.},
