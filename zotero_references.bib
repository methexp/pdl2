  author = {Norman, Elisabeth and Price, Mark C. and Duff, Simon C. and Mentzoni, Rune A.},
  urldate = {2014-10-30},
  date = {2007-12},
  pages = {809--837},
  keywords = {Consciousness,Fringe consciousness,Implicit learning,Individual differences,Personality,Sequence learning,Serial reaction time task},
  file = {ScienceDirect Full Text PDF:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/AUHSMZPV/Norman et al. - 2007 - Gradations of awareness in a modified sequence lea.pdf:application/pdf}
