  keywords = {2012,Cluster Analysis,clustering,Episodic memory,Long Term Memory,sequence memory,Short-term memory,Short Term Memory,working memory},
  file = {EBSCO Full Text:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/PBAS5AHM/Farrell - 2012 - Temporal clustering and sequencing in short-term m.pdf:application/pdf},
