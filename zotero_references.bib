  pages = {1047--1060},
  keywords = {adults,characteristics & development of procedural vs declarative memory,learning,Memory},
  file = {EBSCO Full Text:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/3Q9VKB47/Willingham et al. - 1989 - On the development of procedural knowledge.pdf:application/pdf}
}

