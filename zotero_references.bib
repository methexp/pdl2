  pages = {508--517},
  keywords = {Cognitive Psychology},
  file = {Buchner_etal_1997.pdf:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/GK4DG9SB/Buchner_etal_1997.pdf:application/pdf;Full Text PDF:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/WHXTIT8E/Buchner et al. - The nature of memory processes underlying recognit.pdf:application/pdf}
