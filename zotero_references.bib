  pages = {43--63},
  keywords = {Cognitive Ability,event-coding theory,executive control,Implicit and explicit knowledge,Individual differences,Sequential Learning},
  file = {EBSCO Full Text:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/U79NQHX7/Tubau et al. - 2007 - Modes of executive control in sequence learning F.pdf:application/pdf}
