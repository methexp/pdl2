@article{hurlstone_memory_2014,
  title = {Memory for Serial Order across Domains: {{An}} Overview of the Literature and Directions for Future Research},
  volume = {140},
  issn = {0033-2909},
  url = {http://search.ebscohost.com/login.aspx?direct=true&db=pdh&AN=2013-34043-001&site=ehost-live},
  doi = {10.1037/a0034221},
  shorttitle = {Memory for Serial Order across Domains},
  abstract = {From vocabulary learning to imitating sequences of motor actions, the ability to plan, represent, and recall a novel sequence of items in the correct order is fundamental for many verbal and nonverbal higher level cognitive activities. Here we review phenomena of serial order documented across the verbal, visual, and spatial short-term memory domains and interpret them with reference to the principles of serial order and ancillary assumptions instantiated in contemporary computational theories of memory for serial order. We propose that functional similarities across domains buttress the notion that verbal, visual, and spatial sequences are planned and controlled by a competitive queuing (CQ) mechanism in which items are simultaneously active in parallel and the strongest item is chosen for output. Within the verbal short-term memory CQ system, evidence suggests that serial order is represented via a primacy gradient, position marking, response suppression, and cumulative matching. Evidence further indicates that output interference operates during recall and that item similarity effects manifest during both serial order encoding and retrieval. By contrast, the principles underlying the representation of serial order in the visual and spatial CQ systems are unclear, largely because the relevant studies have yet to be performed. In the spatial domain, there is some evidence for a primacy gradient and position marking, whereas in the visual domain there is no direct evidence for either of the principles of serial order. We conclude by proposing some directions for future research designed to bridge this and other theoretical gaps in the literature. (PsycINFO Database Record (c) 2014 APA, all rights reserved). (journal abstract)},
  timestamp = {2016-11-24T12:52:31Z},
  number = {2},
  journaltitle = {Psychological Bulletin},
  shortjournal = {Psychological Bulletin},
  author = {Hurlstone, Mark J. and Hitch, Graham J. and Baddeley, Alan D.},
  urldate = {2016-11-17},
  date = {2014-03},
  pages = {339--373},
  note = {00071},
  keywords = {2014,competitive queuing,computational modeling,computational models,domains,serial order,Serial Position Effect,serial recall,Short-term memory,Short Term Memory,Spatial Memory,Verbal Memory,verbal & visual & spatial memory,Visual Memory},
  file = {EBSCO Full Text:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/DQQJM2S5/Hurlstone et al. - 2014 - Memory for serial order across domains An overvie.pdf:application/pdf},
  groups = {Context}
}

