  date = {2014-01-01},
  pages = {50--56},
  keywords = {artificial grammar learning,assessment,Consciousness,measures,Sequence learning},
  file = {Gaillard_etal_2014.pdf:/home/mariusbarth/.zotero/zotero/o8fzc1uv.default/zotero/storage/W8V5EJIZ/Gaillard_etal_2014.pdf:application/pdf},
  eprinttype = {pmid},
